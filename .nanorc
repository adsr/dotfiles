unset backup
set const
set morespace
set multibuffer
set nohelp
set regexp
set smarthome
set smooth
set tabsize "4"
set tabstospaces
set nowrap

include "/usr/share/nano/nanorc.nanorc"
include "/usr/share/nano/python.nanorc"
include "/usr/share/nano/ruby.nanorc"
include "/usr/share/nano/c.nanorc"
include "/usr/share/nano/perl.nanorc"

syntax "php" "\.(php[2345s~]?|tpl)$"
color green "\$[a-zA-Z_0-9$]*|[=!<>-]"
color yellow "\b(class|var|const|static|final|private|public|protected|function|switch|case|default|endswitch|if|else|elseif|endif|for|foreach|endfor|endforeach|while|endwhile|new|die|exit|echo|continue|break|include|require|include_once|require_once|return|abstract|interface)\b"
color brightred "[(){}.,;:?!]"
color brightred "[+=/%-]"
color brightblue "\b-?[0-9]+\b"
color brightblue "\b(true|false|null)\b"
color brightgreen "\$"
color brightgreen "=>"
color brightgreen "->"
color brightgreen "::"
color brightred "\["
color brightred "\]"
color brightyellow "'([^']|(\\'))*'"
color brightyellow ""([^"]|(\\"))*""
color cyan "//.*$"
color cyan start="/\*" end="\*/"
color green "(<\?(php)?|\?>)"
color white start="\?>" end="<\?(php)?"
color ,green "[[:space:]]+$"

syntax "js" "\.js$"
color white "^.+$"
color brightblue "\<[-+]?([1-9][0-9]*|0[0-7]*|0x[0-9a-fA-F]+)([uU][lL]?|[lL][uU]?)?\>"
color brightblue "\<[-+]?([0-9]+\.[0-9]*|[0-9]*\.[0-9]+)([EePp][+-]?[0-9]+)?[fFlL]?"
color brightblue "\<[-+]?([0-9]+[EePp][+-]?[0-9]+)[fFlL]?"
color yellow "\<(break|case|catch|continue|default|delete|do|else|finally)\>"
color yellow "\<(for|function|if|in|instanceof|new|null|return|switch)\>"
color yellow "\<(switch|this|throw|try|typeof|undefined|var|void|while|with)\>"
color yellow "\<(Array|Boolean|Date|Enumerator|Error|Function|Math)\>"
color yellow "\<(Number|Object|RegExp|String)\>"
color brightblue "\<(true|false|null)\>"
color brightred "[(){},;:?!]"
color brightred "[+=/%-]"
color brightred "\["
color brightred "\]"
color brightyellow "'([^']|(\\'))*'"
color brightyellow ""([^"]|(\\"))*""
color cyan start="/\*" end="\*/"
color cyan "//.*$"

syntax "java" "\.java$"
color green "\<(boolean|byte|char|double|float|int|long|new|short|this|transient|void)\>"
color red "\<(break|case|catch|continue|default|do|else|finally|for|if|return|switch|throw|try|while)\>"
color cyan "\<(abstract|class|extends|final|implements|import|instanceof|interface|native|package|private|protected|public|static|strictfp|super|synchronized|throws|volatile)\>"
color red ""[^"]*""
color yellow "\<(true|false|null)\>"
color blue "//.*"
color blue start="/\*" end="\*/"
color brightblue start="/\*\*" end="\*/"

syntax "go" "\.go$"
color green "\<((u)?int(8|16|32|64)?|float(32|64)|byte|string|interface|nil|bool|true|false)\>"
color green "\<(package|import|const|var|type|struct|func)\>"
color brightyellow "\<(for|range|if|else|case|default|switch)\>"
color magenta "\<(continue|break|return)\>"
color brightyellow "".*""
color brightyellow "'.'"
color brightblue "//.*"
color brightblue start="/\*" end="\*/"
color ,green "[[:space:]]+$"


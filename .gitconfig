[user]
    name = Adam Saponara
    email = as@php.net
[github]
    user = adsr
[alias]
    br = branch
    ci = commit
    co = checkout
    df = diff
    dfs = diff --staged
    dfm = diff origin/master master
    l = log --graph --decorate
    rpull = pull --rebase --stat
    st = status
    un = reset
    praise = blame
[color]
    ui = auto
[core]
    autocrlf = input
    safecrlf = true
    excludesfile = ~/.gitignore
    preloadindex = true
[push]
    default = tracking
[credential]
    helper = cache --timeout=15552000
